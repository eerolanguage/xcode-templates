<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>AllowedTypes</key>
    <array>
        <string>org.eerolanguage.eero-source</string>
    </array>
    <key>Description</key>
    <string>An Eero category, with implementation and header files.</string> 
    <key>Kind</key>
    <string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
    <key>MainTemplateFile</key>
    <string>___FILEBASENAME___.eero</string>
    <key>Options</key>
    <array>
        <dict>
            <key>Description</key>
            <string>The name of the category to create</string>
            <key>Identifier</key>
            <string>categoryName</string>
            <key>Name</key>
            <string>Category</string>
            <key>NotPersisted</key>
            <true/>
            <key>Required</key>
            <true/>
            <key>Type</key>
            <string>text</string>
        </dict>
        <dict>
            <key>Default</key>
            <string></string>
            <key>Description</key>
            <string>Which class to add a category to</string>
            <key>Identifier</key>
            <string>categoryClass</string>
            <key>Name</key>
            <string>Category on</string>
            <key>NotPersisted</key>
            <true/>
            <key>Required</key>
            <true/>
            <key>Type</key>
            <string>text</string>
        </dict>
        <dict>
            <key>Default</key>
            <string>___VARIABLE_categoryClass:identifier___+___VARIABLE_categoryName:identifier___</string>
            <key>Identifier</key>
            <string>productName</string>
            <key>Type</key>
            <string>static</string>
        </dict>
    </array>
    <key>Platforms</key>
    <array>
      <string>com.apple.platform.iphoneos</string>
      <string>com.apple.platform.macosx</string>
    </array>
    <key>Summary</key>
    <string>An Eero category with a header</string>
</dict>
</plist>
